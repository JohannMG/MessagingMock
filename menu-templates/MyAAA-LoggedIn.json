    {
        "Id": "MyAAA/MembershipLevels/Classic",
        "Title": "Classic membership",
        "Groups": [
            {
                "Text": null,
                "Content": [
                    {
                        "Type": "HTML",
                        "Html": [
                            "<div class='membership_level_details'>",
                            "   <img src='ClassicBanner.jpg' />",
                            "   <h1>Benefits of AAA<br/>Classic Membership</h1>",
                            "   <aside>",
                            "       $66.00<br/>",
                            "       <button>Join AAA</button>",
                            "   </aside>",
                            "   <p>Classic membership gives you access to the #1-rated roadside assistance service in the nation, incredible savings with the largest member discount program in the world, and more.</p>",
                            "   <p>First additional member: <b>1 year free</b></p>",
                            "   <p>Each additional member: <b>$15 for the first year</b></p>",
                            "   <hr/>",
                            "   <h2>Auto Rescue</h2>",
                            "   <ul>",
                            "       <li>Up to four 5-mile tows</li>",
                            "       <li>Emergency starting</li>",
                            "       <li>Free mobile battery service</li>",
                            "       <li>Flat tire service</li>",
                            "       <li>Fuel service delivery - you pay fuel</li>",
                            "       <li>Vehicle locksmith, up to $50 for lockout only</li>",
                            "       <li>Extrication/winching</li>",
                            "   </ul>",
                            "   <h2>Travel</h2>",
                            "   <ul>",
                            "       <li>Free access to AAA TripTik travel planner and AAA TourBook guides</li>",
                            "       <li>AAA approved lodging and hotel ratings</li>",
                            "       <li>Nominal fee for Passport photos</li>",
                            "       <li>Car travel interruption up to $600</li>",
                            "       <li>Free travel accident insurance for AAA travel up to $100,000</li>",
                            "       <li>Free Hertz gold plus rewards membership + 675 Hertz rewards points after first gold rental</li>",
                            "   </ul>",
                            "   <h2>Bonus Benefits</h2>",
                            "   <ul>",
                            "       <li>Free emergency check cashing</li>",
                            "       <li>Everyday savings at more than 100,000 partner locations worldwide</li>",
                            "       <li>Free insurance policy review and quotes</li>",
                            "       <li>Legal service fee reimbursement:<br/>",
                            "           Manslaughter - $500<br/>",
                            "           Reckless driving - $250<br/>",
                            "           Minor traffic violations - $100",
                            "       </li>",
                            "   </ul>",
                            "   <footer>",
                            "       <button>Join AAA now</button>",
                            "   </footer>",
                            "</div>"
                        ]
                    }
                ]
            }
        ]
